package com.agent.czb.core.sys.model;

import java.io.Serializable;

/**
 * 文件上传模型层
 */
public class FileUpdateInfoModel implements Serializable { // 文件上传
	private Long id;//日志ID
	private Long userId;//用户ID
	private String fileType;//文件类型，file，img
	private String fileName;//文件名称
	private String fileUrl;//文件URL
	private String fileRvtype;//图片关联类型
	private Long fileRvid;//图片关联ID
	private Integer fileRank;//文件顺序
	private String state;//状态(0：表示删除；1：表示正常)
	private java.util.Date createDate;//创建时间
	private String note;//说明

	public Long getId() {
		return id;
	}

 	public void setId(Long id) {
		this.id = id;
	}

	public Long getUserId() {
		return userId;
	}

 	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public String getFileType() {
		return fileType;
	}

 	public void setFileType(String fileType) {
		this.fileType = fileType;
	}

	public String getFileName() {
		return fileName;
	}

 	public void setFileName(String fileName) {
		this.fileName = fileName;
	}

	public String getFileUrl() {
		return fileUrl;
	}

 	public void setFileUrl(String fileUrl) {
		this.fileUrl = fileUrl;
	}

	public String getFileRvtype() {
		return fileRvtype;
	}

 	public void setFileRvtype(String fileRvtype) {
		this.fileRvtype = fileRvtype;
	}

	public Long getFileRvid() {
		return fileRvid;
	}

 	public void setFileRvid(Long fileRvid) {
		this.fileRvid = fileRvid;
	}

	public Integer getFileRank() {
		return fileRank;
	}

 	public void setFileRank(Integer fileRank) {
		this.fileRank = fileRank;
	}

	public String getState() {
		return state;
	}

 	public void setState(String state) {
		this.state = state;
	}

	public java.util.Date getCreateDate() {
		return createDate;
	}

 	public void setCreateDate(java.util.Date createDate) {
		this.createDate = createDate;
	}

	public String getNote() {
		return note;
	}

 	public void setNote(String note) {
		this.note = note;
	}


}
